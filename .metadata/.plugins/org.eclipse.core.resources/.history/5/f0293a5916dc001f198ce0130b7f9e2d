package Jan26;

import java.util.Scanner;

public class AlternatePrime {
public static void main(String[] args) {
	System.out.println("ENter a range to print Alternate prime number ");
	Scanner sc = new Scanner(System.in);
	System.out.println("Enter the start range");
	int st = sc.nextInt();
	System.out.println("Enter the end range");
	int end = sc.nextInt();
}
static void prime(int st,int end,int count) {
	if(st>end)return;
	if(isPrime(st, st/2)) {
		count++;
		if(count%2!=0) {
			System.out.print(st+" ");
		}
	}
	prime(st+1, end, count);
}
static boolean isPrime(int n,int i) {
	if(n<=1)return false;
	if(i==1)return true;
	if(n%i==0) {
		return false;
	}
	return isPrime(n, i-1);
	
}
}
