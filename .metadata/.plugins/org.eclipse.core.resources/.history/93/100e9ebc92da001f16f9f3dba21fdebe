package Jan25;

import java.util.Scanner;

public class Fibseries {
public static void main(String[] args) {
	System.out.println("ENter the range");
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
	fib(0, 1, 1, n);
}
static void fib(int a,int b,int count,int n) {
	if(count>n) {
		return;
	}
	System.out.print(a + " ");
	fib(b, a+b, count+1, n);
}
}
