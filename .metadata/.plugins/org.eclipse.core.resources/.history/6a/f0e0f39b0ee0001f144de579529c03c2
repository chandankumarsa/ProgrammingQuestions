package Jan27;

import java.util.Scanner;

public class Feb1 {
public static void main(String[] args) {
	System.out.println("ENter the start range and end range");
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
	
}
static boolean isPrime(int n,int i) {
	if(n<=1)return false;
	if(i==1)return true;
	if(n%i==0) {
		return false;
	}
	return isPrime(n, i-1);
}
}
