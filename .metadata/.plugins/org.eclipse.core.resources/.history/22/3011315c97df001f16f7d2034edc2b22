package Jan31;

import java.util.Scanner;

public class AutomorphcNumber {
public static void main(String[] args) {
	System.out.println("ENter a Number to check if the number is Automorphic number or not");
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
	int sq = n*n;
	boolean flag = true;
	while(n>0) {
		if(n%10 !=sq%10) {
			flag = false;
			break;
		}
		n = n/10;
		sq = sq/10;
	}
	if(flag) {
		System.out.println("is a Automorphic number");
	}
	System.out.println("is not a Automorphic number");
}
}
