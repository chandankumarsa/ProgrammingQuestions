package Jan31;

import java.util.Scanner;

public class Prime {
public static void main(String[] args) {
	System.out.println("ENter a Number to chec Prime number or not");
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
	System.out.println(isPrime(n/10, n));
}
static boolean isPrime(int n,int i) {
	if(n<=1)return false;
	if(i==1)return true;
	if(n%i==0)return false;
	return isPrime(n/10, i-1);
}
}

