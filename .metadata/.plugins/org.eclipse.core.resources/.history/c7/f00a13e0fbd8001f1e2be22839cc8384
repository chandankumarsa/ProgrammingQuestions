package Jan22;

import java.util.Scanner;

public class SumOfDigit {
	public static void main(String[] args) {
		System.out.println("Enter a NUmber to Find Sum oF DIgit");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
	}
	static int sum(int n) {
		if(n==0) {
			return 0;
		}
		return n+sum(n/10);
	}

}
