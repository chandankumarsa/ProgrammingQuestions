package Jan22;

import java.util.Scanner;

public class HcfOfTwoNumber {
	public static void main(String[] args) {
		System.out.println("Enter Two NUmber to Find HCf");
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
	}
	static int findHcf(int a,int b) {
		if(b==0) {
			return a;
		}
	}

}
