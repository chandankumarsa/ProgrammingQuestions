package Jan29;

import java.util.Scanner;

public class countDIgit {
public static void main(String[] args) {
	System.out.println("ENter a Number to count Digit: ");
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
}
static int count(int n) {
	if(n==0)return 0;
	return 1+count(n/10);
}
}
