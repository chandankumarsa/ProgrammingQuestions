package Jan29;

import java.util.Scanner;

public class Factorial {
public static void main(String[] args) {
	System.out.println("Enter a Number to find the factorial");
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
}
static int fact(int n) {
	if(n==0 || n==1) {
		return 1;
	}
	return n*fact(n/10);
}
}
