class A{
	A(){
		this(60);
		System.out.println();
}
	A(int i){
		System.out.println("A (int i) cons "+i);
	}
}
class B extends A{
	B(){
		super(50);
		System.out.println("B () cons");
	}
	B(int i){
		this();
		System.out.println("B (int i)cons : "+i);
	}
}
class C extends B{
	C(){
		this(10);
		System.out.println("c() cons");
	}

 C(int i) {
		super(i);
	}
}
public class Demo {
	public static void main(String[] args) {
		
	}

}
