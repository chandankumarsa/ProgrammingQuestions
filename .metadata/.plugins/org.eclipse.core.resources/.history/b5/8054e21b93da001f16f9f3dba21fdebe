package Jan25;

import java.util.Scanner;

public class FibSeries {
	public static void main(String[] args) {
		System.out.println("ENter the ranges");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
	}
	static void fib(int a,int b,int count,int n) {
		if(count>n) {
			return;
		}
		System.out.print(a+" ");
		fib(a, b, count, n);
	}

}
