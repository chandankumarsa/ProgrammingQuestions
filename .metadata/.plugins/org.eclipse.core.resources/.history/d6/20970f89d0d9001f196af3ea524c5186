package Jan23;

import java.util.Scanner;

public class FibSeries {
    public static void main(String[] args) {
        System.out.println("Enter range");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        fib(0, 1, 1, n);  // Start Fibonacci series with 0 and 1, count 1, up to n
    }

    static void fib(int a, int b, int count, int n) {
        if (count > n) {  // Base case: stop when count exceeds n
            return;
        }
        System.out.print(a + " ");  // Print the current Fibonacci number
        fib(b, a + b, count + 1, n);  // Recursive call for next Fibonacci numbers
    }
}
