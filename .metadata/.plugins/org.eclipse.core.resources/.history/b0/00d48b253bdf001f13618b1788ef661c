import java.util.Scanner;
class A{
	A(){
		System.out.println("A () cons");
	}
}
class B extends A{
	B(){
		System.out.println("B () cons");
	}
}
class C extends B{
	C(){
		System.out.println("C() cons");
	}
}
public class Demo1 {
	public static void main(String[] args) {
		new A();
		new B();
		new C();
	}

}
