package Jan23;
class Rectangle{
	int length;
	int breadth;
	Rectangle(int l,int b){
		length=l;
		breadth=b;
		Rectangle(Rectangle r){
			length = r.length;
			breadth = b.breadth;
		}
		void display() {
			System.out.println("length"+length+",breadth = "+breadth);
		}
	}
	
}
