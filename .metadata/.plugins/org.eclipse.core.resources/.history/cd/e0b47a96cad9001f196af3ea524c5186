package Jan23;

import java.util.Scanner;

public class Print1ton {
	public static void main(String[] args) {
		System.out.println("Enter a NUmber to Print 1 to n");
		Scanner sc = new Scanner (System.in);
		int n = sc.nextInt();
	}
	static int priint(int n) {
		if(n==0) {
			return 0;
		}
		return priint(n-1);
	}

}
