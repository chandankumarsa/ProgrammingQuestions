package Jan27;

import java.util.Scanner;

public class HighestPrimeNumber {
	public static void main(String[] args) {
		System.out.println("Enter the Start and End range  to print Highest prime Number");
		Scanner sc = new Scanner(System.in);
		int st = sc.nextInt();
		int end = sc.nextInt();
		range(st,end);
	}
	static void range(int st,int end) {
		if(st>end)return;
		if(isPrime(end, end/2)) {
			System.out.println(end + " ");
			return;
		}
		isPrime(st, end-1);
	}
	static boolean isPrime(int n ,int i) {
		if(n<=1) {
			return false;
		}
		if(i==1) {
			return true;
		}
	return isPrime(n, i-1);
	}

}
