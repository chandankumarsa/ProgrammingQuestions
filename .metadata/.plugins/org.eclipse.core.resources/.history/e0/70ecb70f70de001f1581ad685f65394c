	System.out.println("ENter a Number to Print the sum of Even and Odd Number");
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
}
static int esum(int n) {
	if(n==0)return 0;
	int rem = n%10;
	if(rem%2==0) {
		sum+=rem+
	}
	return esum(n, sum)
	
}
}
