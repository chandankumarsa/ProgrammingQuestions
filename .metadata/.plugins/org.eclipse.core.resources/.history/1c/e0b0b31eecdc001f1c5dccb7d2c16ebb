package Jan28;

import java.util.Scanner;

public class SumOfDigitSingleDigit {
	public static void main(String[] args) {
		System.out.println("ENter a NUmber to Print Sum Of DIgit Should Be in Single Digit");
		Scanner sc = new Scanner(System.in);
		int n= sc.nextInt();
		
	}
	
static int sum(int n) {
	if(n==0)return 0;
	return n%10+sum(n/10);
}
}
