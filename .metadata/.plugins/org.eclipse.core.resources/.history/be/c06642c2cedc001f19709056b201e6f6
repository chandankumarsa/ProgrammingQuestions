package Jan27;

import java.util.Scanner;

public class Strong {
	public static void main(String[] args) {
		System.out.println("ENter a Number to checkk if it is a Strong number or not");
		Scanner sc = new Scanner (System.in);
		int n = sc.nextInt();
	}
	static int fact(int n) {
		if(n==0)return 1;
		return n*fact(n-1);
	}

}
