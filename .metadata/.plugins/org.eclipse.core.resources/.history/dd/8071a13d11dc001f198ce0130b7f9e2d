package Jan26;

import java.util.Scanner;

public class PrimeNumberinRange {
public static void main(String[] args) {
	System.out.println("Enter The Start And End Range To Print Prime NUmber");
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
}
static boolean isPrime(int n,int i) {
	if(n<=1)return false;
	if(i==1)return true;
	if(n%i==0) {
		return false;
	}
	return isPrime(n, i)
	
}
}
