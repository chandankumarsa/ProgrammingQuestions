package Practice;

import java.util.Scanner;

public class Fib {
	public static void main(String[] args) {
		System.out.println(" Enter a Number ");
		int n = new Scanner(System.in).nextInt();
		System.out.println(fib(n));
	}
	static int fib(int n) {
		if(n==1) return 0;
		if(n==2)return 1;
		return fib(n-1)+fib(n-2);
	}

}
