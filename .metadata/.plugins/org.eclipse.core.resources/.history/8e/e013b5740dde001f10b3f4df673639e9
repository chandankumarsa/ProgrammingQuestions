package Jan29;

import java.util.Scanner;

public class NthFib1 {
	public static void main(String[] args) {
		System.out.println("ENter  a Number to Print Nth Fib Number");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
	}
	static int fib(int n) {
		if(n==1)return 0;
		if(n==2)return 1;
		return fib(n-1)+fib(n-2);
	}

}
