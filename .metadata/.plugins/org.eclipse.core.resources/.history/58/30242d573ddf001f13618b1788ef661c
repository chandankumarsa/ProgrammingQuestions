package Jan31;

import java.util.Scanner;

public class MultiplicationOfInteger {
public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.println("Enter a Number to find Multiplication of an Integer");
	int n = sc.nextInt();
	System.out.println("Multiplication of an integer is "+mul(n));
}
static int mul(int n) {
	if(n==0 || n==1)return 1; 
	return mul(n);
}
}
