package Jan26;

import java.util.Scanner;

public class PrimeNumberinRange {
	public static void main(String[] args) {
		System.out.println("Enter The Start And End Range To Print Prime NUmber");
		Scanner sc = new Scanner(System.in);
		System.out.println("ENter the start Range");
		int st = sc.nextInt();
		System.out.println("ENter the end Range");
		int end = sc.nextInt();
		prime(st, end);
	}
	static void prime(int st, int end) {
		if (st > end)
			return;
		if (isPrime(st, st / 2)) {
			System.out.print(st +" ");
		}
		prime(st, end);
	}

	static boolean isPrime(int n, int i) {
		if (n <= 1)
			return false;
		if (i == 1)
			return true;
		if (n % i == 0) {
			return false;
		}
		return isPrime(n, i - 1);

	}
}
