package Jan23;

import java.util.Scanner;

public class SumOfEvenOddPrintSeparately {
public static void main(String[] args) {
	System.out.println("ENter a Number to Print Sum of EVen and odd");
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
}
static int esum(int n) {
	if(n==0) {
		return 0;
	}
	int rem =n%10;
	if(rem%2==0) {
		return rem+esum(n/10);
	}
	return esum(n/10);
}
}
