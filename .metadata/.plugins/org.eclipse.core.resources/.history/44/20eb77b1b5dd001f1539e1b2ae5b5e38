package Jan29;

import java.util.Scanner;

public class FibRange {
	public static void main(String[] args) {
		System.out.println("Enter a Number to Print Fib Series in a Range");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		fib(0, 1, 1, n);
	}
	static void fib(int a,int b,int count,int n) {
		if(count>n) {
			System.out.print(a+" ");
		}
		fib(b, a+b, count+1, n);
	}

}
