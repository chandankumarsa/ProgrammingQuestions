package Feb1;

import java.util.Scanner;

public class AlternatePrime1 {
	public static void main(String[] args) {
		System.out.println("Enter a Number to print Prime NUmber in a Range");
		Scanner sc = new Scanner(System.in);
		int st  = sc.nextInt();
		int end =sc.nextInt();
		range(st,end);
	}
	static void range(int st,int end,int count,int n) {
		if(st>end)return;
		if(isPrime(st, st/2)) {
			count++;
			if(count%2!=0) {
				System.out.print(st+" ");
			}
		}
	}
	static boolean isPrime(int n, int i) {
		if (n <= 1)
			return false;
		if (i == 1)
			return true;
		if (n % i == 0) {
			return false;
		}
		return isPrime(n, i - 1);

	}

}
